<ol>
  <% @songs.each do |song| %>
    <li><%= song.title %></li>
    <p><%= song.release_year if song.released %></p>
    <p><%= song.artist_name %></p>
    <p><%= song.genre %></p>
  
  <% end %>
</ol>

<% @songs.each do |song| %>
<div class="song">
  <span><%= song.title %></span>
  <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
</div>
<% end %>